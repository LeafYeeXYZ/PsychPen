    "echarts-stat": "^1.2.0",
    "echarts-wordcloud": "^2.1.0",
    "html2canvas": "^1.4.1",
    "jieba-wasm": "^2.2.0",
    "ml-kmeans": "^6.0.0",
    "react": "rc",
    "react-dom": "rc",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@types/bun": "^1.1.14",
    "@types/react": "npm:types-react@rc",
