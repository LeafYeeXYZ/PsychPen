{
  "name": "psych-pen",
  "version": "1.7.0",
  "description": "Web statistic app for Psychology and Education",
  "engines": {
    "bun": "^1.1.32"
  },
  "private": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/LeafYeeXYZ/PsychPen.git"
  },
